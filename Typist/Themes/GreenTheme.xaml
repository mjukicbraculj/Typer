<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="buttonStyle">
        <Setter Property="Control.Background" Value="DarkCyan"/>
        <Setter Property="Control.Foreground" Value="Black"/>
    </Style>
    <SolidColorBrush x:Key="childButtonBackground" Color="LightBlue"/>
    <SolidColorBrush x:Key="appBackground" Color="AliceBlue"></SolidColorBrush>
    <SolidColorBrush x:Key="appBorder" Color="DarkCyan"></SolidColorBrush>
    <Style x:Key="headingStyle" TargetType="TextBlock">
        <Setter Property="Background" Value="DarkCyan"/>
        <Setter Property="Foreground" Value="White"/>
    </Style>
</ResourceDictionary>